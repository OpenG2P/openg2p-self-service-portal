<odoo>
    <record id="view_spp_program_reimbursement_form" model="ir.ui.view">
        <field name="name">view_spp_program_reimbursement_form</field>
        <field name="model">g2p.program</field>
        <field name="inherit_id" ref="g2p_self_service_portal.self_service_portal_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='file_size_ssp']" position="after">
                <field name="file_size_spp" string="Service Provider Portal (MB)" />
            </xpath>
        </field>
    </record>

</odoo>
